<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_work_order" model="ir.sequence">
            <field name="name">Work Order</field>
            <field name="code">crane.work.order</field>
            <field name="prefix">WO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_task" model="ir.sequence">
            <field name="name">Task</field>
            <field name="code">crane.task</field>
            <field name="prefix">T</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_certificate" model="ir.sequence">
            <field name="name">Certificate Number</field>
            <field name="code">crane.equipment</field>
            <field name="prefix">HD</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
