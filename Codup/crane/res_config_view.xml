<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="action_client_crane_menu" model="ir.actions.client">
            <field name="name">Open Crane Menu</field>
            <field name="tag">reload</field>
            <field name="params" eval="{'menu_id': ref('base.menu_crane_root')}"/>
        </record>
        <record id="base.open_menu" model="ir.actions.todo">
            <field name="action_id" ref="action_client_crane_menu"/>
            <field name="state">open</field>
        </record>
    </data>
</openerp>
